contract AccessControl {
    mapping(address => bool) private blacklist;
    mapping(address => bool) private whitelist;

    constructor() {
        // Team wallets that will be moving funds to Liquidity, burning big quantities of tokens,
        // movements which could be suspicious for the anti dump system.
        addWhitelist(address(0xA7Ef38cB0DC82066Ec3a37106e916C6BCe2c7744));

        // Known malicious wallets that snipe and dump, mostly mempool bots.
        addBlacklist(address(0x17983e1766F6B5646190EcF6554ABD8fd00cfC5A));
        addBlacklist(address(0x5A4187616425B256a7fe7ed9022453aF1e6f666A));
    }

    function addBlacklist(address someone) internal {
        blacklist[someone] = true;
    }

    function removeBlacklist(address someone) internal {
        blacklist[someone] = false;
    }

    function addWhitelist(address someone) internal {
        blacklist[someone] = true;
    }

    function removeWhitelist(address someone) internal {
        blacklist[someone] = false;
    }

    function isWhitelisted(address someone) public view returns (bool) {
        return whitelist[someone];
    }

    function isBlacklisted(address someone) public view returns (bool) {
        return blacklist[someone];
    }
}

